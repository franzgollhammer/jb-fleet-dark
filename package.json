{
  "name": "jb-fleet-dark",
  "displayName": "VS Code Fleet",
  "version": "1.0.12",
  "description": "VS Code Fleet inspired by JetBrains Fleet Dark Theme",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Theme",
    "Dark Theme",
    "Fleet",
    "VS Code Fleet",
    "VS Code Fleet Dark",
    "Jetbrains",
    "JetBrains Fleet Dark",
    "Jet Brains",
    "Jet Brains Fleet",
    "Sphere"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/franzgollhammer/jb-fleet-dark"
  },
  "license": "MIT",
  "author": {
    "name": "Franz Gollhammer"
  },
  "publisher": "franzgollhammer",
  "scripts": {
    "ncu": "npx npm-check-updates -u",
    "package": "npx vsce package",
    "publish": "npm run upgrade-package-version && npm run sort-package && npm run package && npx vsce publish",
    "sort-package": "npx sort-package-json ",
    "upgrade-package-version": "node ./scripts/upgrade-package-version.js"
  },
  "contributes": {
    "themes": [
      {
        "label": "Fleet",
        "uiTheme": "vs-dark",
        "path": "./themes/Fleet-color-theme.json"
      },
      {
        "label": "Fleet Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Fleet Dark-color-theme.json"
      },
      {
        "label": "Fleet Grey",
        "uiTheme": "vs-dark",
        "path": "./themes/Fleet Grey-color-theme.json"
      },
      {
        "label": "Fleet Ocean",
        "uiTheme": "vs-dark",
        "path": "./themes/Fleet Ocean-color-theme.json"
      },
      {
        "label": "Fleet Palenight",
        "uiTheme": "vs-dark",
        "path": "./themes/Fleet Palenight-color-theme.json"
      },
      {
        "label": "Sphere",
        "uiTheme": "vs-dark",
        "path": "./themes/Sphere-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "icon": "icon.png"
}